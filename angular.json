{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "infoAidTech": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "src/main.ts",
            "server": "src/main.server.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              { "glob": "**/*", "input": "public" },
              { "glob": "**/*", "input": "src/assets", "output": "assets" }
            ],
            "styles": ["src/custom-theme.scss", "src/styles.css"],
            "ssr": {
              "entry": "src/server.ts"
            },
            "outputPath": "dist/infoAidTech"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "700kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all",
              "optimization": true
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "options": {
            "port": 5000
          },
          "configurations": {
            "production": {
              "buildTarget": "infoAidTech:build:production"
            },
            "development": {
              "buildTarget": "infoAidTech:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "serve-ssr": {
          "builder": "@angular/build:ssr-dev-server",
          "options": {
            "browserTarget": "infoAidTech:build:development",
            "serverTarget": "infoAidTech:build:development"
          },
          "configurations": {
            "production": {
              "browserTarget": "infoAidTech:build:production",
              "serverTarget": "infoAidTech:build:production"
            }
          }
        },
        "prerender": {
          "builder": "@angular/build:prerender",
          "options": {
            "routes": ["/", "/home", "/blog"]
          },
          "configurations": {
            "production": {
              "buildTarget": "infoAidTech:build:production"
            }
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
